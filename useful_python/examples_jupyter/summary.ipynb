   "source": [
    "# 字典推导式\n",
    "from random import randint\n",
    "\n",
    "d = {x:randint(50, 100) for x in range(1,10)}\n",
    "print(d)\n",
    "# 选出值为80以上的数据\n",
    "r = {k:v for k, v in d.items() if v > 90}\n",
    "print(r)\n"
   ]
