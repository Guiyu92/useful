    "\n",
    "# 提取文件名\n",
    "d = ret.headers['content-disposition']\n",
    "fname = re.findall(\"filename=(.+)\", d)[0]\n",
    "fname = unquote(fname)\n",
    "fname = '*\\:?\"|/<>'\n",
    "fname = re.sub(r'\\*|\\\\|:|\\?|\"|\\||\\<|\\>|\\/', '_', fname)\n",
    "fname = fname.replace(\"\\\"\", \"\")\n"
   ]
  }
